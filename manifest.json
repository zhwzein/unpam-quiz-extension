{
    "manifest_version": 3,
    "name": "Mentari Unpam Quiz Data Fetcher",
    "version": "1.0",
    "description": "Fetch quiz data from Mentari UNPAM API",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://mentari.unpam.ac.id/*",
        "https://node.andaraz.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icon/icon128.png"
        }
    }
}